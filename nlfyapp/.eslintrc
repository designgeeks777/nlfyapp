{
  "extends": "@react-native-community",
  "rules": {
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "babelOptions": {
      "parserOpts": {
        "plugins": ["jsx"]
      }
    },
    "requireConfigFile": false
  }
}
